                "invalid"
            ],
            "settings": {
                "background": "#ff6767",
                "foreground": "#15141b"
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": [
                "invalid.deprecated"
            ],
            "settings": {
                "background": "#a277ff",
                "foreground": "#15141b"
            }
        },
        {
            "name": "Markup Heading",
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "foreground": "#ff6767"
            }
        },
        {
            "name": "Markup Underline",
            "scope": [
                "markup.underline"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markup Bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffca85"
            }
        },
        {
            "name": "Markup Italic",
            "scope": [
                "markup.italic"

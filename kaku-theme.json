    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#6b6b6b"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#61ffca"
            }
        },
        {
            "name": "String Escape",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#ffca85"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#ffca85"
            }
        },
        {
            "name": "Built-in constant",
            "scope": [
                "constant.language"
            ],
            "settings": {
                "foreground": "#ff6767"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#ff6767"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable"
            ],
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#a277ff"
            }
        },
        {
            "name": "Storage",
            "scope": [
                "storage"
            ],
            "settings": {
                "foreground": "#a277ff"
            }
        },
        {
            "name": "Storage type",
            "scope": [
                "storage.type"
            ],
            "settings": {
                "foreground": "#a277ff"
            }
        },
        {
            "name": "Class name",
            "scope": [
                "entity.name.class"
            ],
            "settings": {
                "foreground": "#61ffca",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#a277ff"
            }
        },
        {
            "name": "Function argument",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ffca85"
            }
        },
        {
            "name": "Tag name",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#ff6767"
            }
        },
        {
            "name": "Tag attribute",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#61ffca"
            }
        },
        {
            "name": "Library function",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#61ffca"
            }
        },
        {
            "name": "Library constant",
            "scope": [
                "support.constant"
            ],
            "settings": {
                "foreground": "#61ffca"
            }
        },
        {
            "name": "Library class/type",
            "scope": [
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#a277ff"
            }
        },
        {
            "name": "Library variable",
            "scope": [
                "support.other.variable"
            ],
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Invalid",
